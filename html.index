      background-position: center;
      background-attachment: fixed;
      font-family: Arial, sans-serif;
      color: white;
      text-align: center;
    }

    .music-player {
      position: fixed;
      top: 10px;
      left: 10px;
      background: rgba(0, 0, 0, 0.6);
      padding: 10px;
      border-radius: 8px;
      z-index: 1000;
      font-size: 14px;
    }

    .music-player audio {
      width: 100%;
      margin-top: 5px;
    }

    .content {
      margin-top: 150px;
      padding: 20px;
    }

    @media (max-width: 600px) {
      .music-player {
        width: 90%;
        left: 5%;
        font-size: 16px;
      }
    }

    @media (min-width: 601px) {
      .music-player {
        width: 300px;
      }
    }
  </style>
</head>
